  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --declaration --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "npm run build",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx --fix"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^24.1.0",
