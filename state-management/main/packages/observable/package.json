{
  "name": "@qtpy/state-management-observable",
  "version": "0.2.6",
  "type": "module",
  "author": "QtPy",
  "license": "MIT",
  "homepage": "https://github.com/TheOnlyFastCoder2/QtPy-library",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
